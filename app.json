{
  "expo": {
    "name": "Healthy Corners Rewards",
    "slug": "healthy-corners-rewards",
    "privacy": "public",
    "sdkVersion": "37.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.1",
    "primaryColor": "#008550",
    "orientation": "portrait",
    "icon": "./assets/images/CustomerAppIcon.png",
    "splash": {
      "image": "./assets/images/DCHC_Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "org.calblueprint.HealthyCornersRewards",
      "buildNumber": "1.0.1",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "To find nearby stores and see what products are available.",
        "LSApplicationQueriesSchemes": [
          "comgooglemaps",
          "citymapper",
          "uber",
          "lyft",
          "waze"
        ]
      }
    },
    "android": {
      "package": "org.calblueprint.HealthyCornersRewards",
      "googleServicesFile": "./google-services.json",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps"
        }
      ]
    },
    "description": ""
  }
}
